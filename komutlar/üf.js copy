const Discord = require("discord.js");
const google = require("google-tts-api");
const ayarlar = require('../ayarlar.json');
const ytdl = require('ytdl-core');
let prefix = ayarlar.prefix;
exports.run = (client, message) => {

    const voiceChannel = message.member.voiceChannel;
    if (!voiceChannel) return message.channel.send(`İlk önce bir sesli kanala girmeniz gerek`)
    voiceChannel.connection.playStream(ytdl('https://youtu.be/vLljFmx7yF8?list=PLezG3_d3HkYpHXnPP0it_Go8ENYlELbWb', { filter: 'audioonly' }));

};
exports.conf = {
    enabled: true,
    guildOnly: false,
    aliases: ['uf',],
    permLevel: 1
};

exports.help = {
    name: 'uf',
    description: 'uf',
    usage: 'uf'
};